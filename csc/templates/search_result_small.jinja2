{% if users %}
    <div>
        <div>Users</div>
        {% for user in users %}
            <div><a href="{{ url_for('user', id=user._id) }}">{{ user.name }}</a></div>
        {% endfor %}
    </div>
{% endif %}
{% if clubs %}
    <div>
        <div>Clubs</div>
        {% for club in clubs %}
            <div><a href="{{ url_for('club', id=club._id) }}">{{ club.name }}</a></div>
        {% endfor %}
    </div>
{% endif %}
{% if posts %}
    <div>
        <div>Posts</div>
        {% for post in posts %}
            <div><a href="{{ url_for('post', id=post._id) }}">{{ post.name }}</a></div>
        {% endfor %}
    </div>
{% endif %}
{% if schools %}
    <div>
        <div>Schools</div>
        {% for school in schools %}
            <div><a href="{{ url_for('school', id=school._id) }}">{{ school.name }}</a></div>
        {% endfor %}
    </div>
{% endif %}
{% if not users and not clubs and not posts and not schools %}
    <div>Sorry, nothing to show</div>
{% endif %}
<hr>