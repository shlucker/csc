<div>
{% if users %}
    <div>
        <div>Users</div>
        {% for user in users %}
            <div><p value="{{user._id }}">{{ user.name }}</p></div>
        {% endfor %}
    </div>
{% endif %}
{% if clubs %}
    <div>
        <div>Clubs</div>
        {% for club in clubs %}
            <div><p value="{{ club._id }}">{{ club.name }}</p></div>
        {% endfor %}
    </div>
{% endif %}
{% if not users and not clubs %}
    <div>Sorry, nothing to show</div>
{% endif %}
<hr>
</div>